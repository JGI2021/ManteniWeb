<table>

  <general
    DisplayName="Páginas con chat"
    Database="TIPHONE"
    TableName="CHAT_PAGINAS">
  </general>

  <panels>
    <panel
      id="panelgenerales"
      DisplayName="Datos Generales">
    </panel>
    <panel
      id="AsociarCanalServicio"
      DisplayName="Canal - Servicio">      
    </panel>
  </panels>

  <fields>

    <field>
      <fattr
        FieldName="IdPagina"
        Panel="panelgenerales"
        DisplayName="Identificador"
        IsKey="true"
        ReadOnly="true"
        EditionType="AUTOGENERATEDINT"
        Mandatory="true"
        ValidateAs="Integer"
        ToolTip="Identificador de la página."
        External="false"
        TabOrder="1"
        Linea ="L1" >
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="IdWebSite"
        Panel="panelgenerales"
        DisplayName="Id website"
        EditionType="Combo"
        Mandatory="true"
        ValidateAs="Alpha"
        External="true"
        Linea="L1"
        UpdateNeeded="true"
        ReadOnly="false"
        TabOrder="2">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
             DatabaseName="TIPHONE"
             TableName="CHAT_WEBSITES"
             FieldKey="IdWebsite"
             FieldValue="Nombre"
             DefaultKey="NULL"
             DefaultValue="Ninguno">
        </table>
      </external>
    </field>

    <field>
      <fattr
        FieldName="AliasPagina"
        Panel="panelgenerales"
        DisplayName="Alias "
        EditionType="Edit"
        MaxLen="100"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Introduzca un alias para la página o ruta."
        External="false"
        Linea="L2"
        UpdateNeeded="true"
        TabOrder="3">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="Pagina"
        Panel="panelgenerales"
        DisplayName="Página o Ruta de páginas"
        Visible="true"
        EditionType="edit"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Introduzca una página o una ruta de páginas"
        External="false"
        MaxLen="200"
        Linea="L2"
        UpdateNeeded="true"
        TabOrder="4">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="ServicioChat_id"
        Panel="panelgenerales"
        DisplayName="Servicio chat"
        EditionType="Combo"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Servicios asociados al grupo"
        External="true"
        UpdateNeeded="true"
        MultipleEdicion="true"
        Linea="L4"
        TabOrder="5">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
             DatabaseName="TIPHONE"
             TableName="CHAT_SERVICIOS"
             FieldKey="SERVICIOCHAT_ID"
             FieldValue="ALIAS"
             DefaultKey="NULL"
             DefaultValue="Ninguno"
      >
        </table>
      </external>
    </field>



    <field>
      <fattr
        FieldName="Activo"
        Panel="panelgenerales"
        DisplayName="Activo "
        EditionType="check"
        Mandatory="false"
        ValidateAs="boolean"       
        External="false"
        InitialValue="true"
        UpdateNeeded="true"
        Linea="L4"
        MultipleEdicion="true"
        TabOrder="10">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="MostrarChat"
        Panel="panelgenerales"
        DisplayName="Mostrar chat "
        EditionType="check"
        Mandatory="false"
        ValidateAs="boolean"       
        External="false"
        InitialValue="true"
        UpdateNeeded="true"
        Linea="L4"
        MultipleEdicion="true"
        TabOrder="11">
      </fattr>
    </field>    

    
  </fields>
</table>

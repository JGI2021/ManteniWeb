<table>

  <general
    DisplayName="Canal"
    Database="TIPHONE"
    TableName="CHANNEL">
  </general>

  <panels>
    <panel
      id="panelgenerales"
      DisplayName="Datos generales">
    </panel>
  </panels>

  <fields>

    <field>
      <fattr
	      FieldName="Channel_type"
        Panel="panelgenerales"
        DisplayName="Código del canal"
	      IsKey="true"
	      ReadOnly="true"
	      EditionType="AUTOGENERATEDINT"
	      Mandatory="true"
	      ValidateAs="Integer"
	      ToolTip="Código del canal"
	      External="false"
	      TabOrder="1">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="Description"
        Panel="panelgenerales"
        DisplayName="Nombre canal"
        EditionType="Edit"
        MaxLen="50"
        Mandatory="true"
        ReadOnly="true"
        ValidateAs="Alpha"
        ToolTip="Nombre del canal"
        External="false"
        UpdateNeeded="false"
        Linea="alias"
        TabOrder="2">
      </fattr>

    </field>


    <field>
      <fattr
        FieldName="Numero_max_Acciones"
        Panel="panelgenerales"
        DisplayName="Número máximo de acciones"
        EditionType="edit"
        Mandatory="false"
        ValidateAs="Integer"
        MaxLen="5"
        ToolTip="Número máximo de acciones por operador."
        External="false"
        Linea="alias"
        UpdateNeeded="true"
        TabOrder="3">
      </fattr>
    </field>
  </fields>
</table>

<table>

  <general
    DisplayName="Etiqueta Chat"
    Database="TIPHONE"
    TableName="CHAT_CALIFICAR_SERVICIO">
  </general>

  <panels>
    <panel
      id="panelgenerales"
      DisplayName="Datos registro">
    </panel>
  </panels>

  <fields>

    <field>
      <fattr
        FieldName="calificar_servicio_id"
        Panel="panelgenerales"
        DisplayName="Id registro"
        IsKey="true"
        ReadOnly="true"
        EditionType="AutoGeneratedInt"
        Mandatory="true"
        ValidateAs="Integer"
        ToolTip="Identificador único del calificador del servicio."
        External="false"
        TabOrder="1"
        Linea="L1"
      >
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="idPagina"
        Panel="panelgenerales"
        DisplayName="Página(s)"
        EditionType="Combo"
        MaxLen="50"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Página o grupo de páginas en las que se puede aplicar la calificación"
        External="true"
        UpdateNeeded="true"
        Linea="L1"
        TabOrder="2">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
             DatabaseName="TIPHONE"
             TableName="CHAT_PAGINAS"
             FieldKey="IDPAGINA"
             FieldValue="ALIASPAGINA"
             DefaultKey="0"
             DefaultValue="SISTEMA">
        </table>
        </external> 
    </field>


    <field>
      <fattr
        FieldName="Mensaje_Calificacion"
        Panel="panelgenerales"
        DisplayName="Texto"
        EditionType="edit"
        Mandatory="true"
        ValidateAs="Alpha"
        MaxLen="250"
        ToolTip="Calificación que se incluirá en el chat."
        External="false"
        Linea="L2"
        UpdateNeeded="true"
        TabOrder="3">
      </fattr>
    </field>


    <field>
      <fattr
        FieldName="Activo"
        Panel="panelgenerales"
        DisplayName="Activo"
        EditionType="check"
        Mandatory="false"
        ValidateAs="boolean"
        ToolTip="Indica si la etiqueta está activa o no"
        External="false"
        Linea="L3"
        UpdateNeeded="true"
        TabOrder="4">
      </fattr>
    </field>

  </fields>
</table>

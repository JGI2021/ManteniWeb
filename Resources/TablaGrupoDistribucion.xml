<table>

  <general
    DisplayName="Grupos ACD"
    Database="TIPHONE"
    SubsystemFieldName="subsistema"
    TableName="GRUPO_ACD"
    TableName2="DISTRIBUCION_TELEFONO"
    CrossField2="GRUPO_ACD_ID">
  </general>

  <panels>
    <panel
      id="panelgenerales"
      DisplayName="Datos Generales">
    </panel>

    <!--<panel
      id="paneltelefonia"
      DisplayName="Telefonía">
    </panel>-->

    <panel
      id="panelprogramas"
      DisplayName="Programas">
    </panel>
  </panels>

  <fields>

    <field>
      <fattr
        FieldName="grupo_acd_id"
        Panel="panelgenerales"
        DisplayName="Identificador"
        IsKey="true"
        ReadOnly="true"
        EditionType="AUTOGENERATEDINT"
        Mandatory="true"
        ValidateAs="Integer"
        Linea="L1"
        ToolTip="Identificador del grupo."
        TabOrder="1">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="alias"
        Panel="panelgenerales"
        DisplayName="Alias"
        EditionType="Edit"
        MaxLen="10"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Teclee un alias para este grupo."
        UpdateNeeded="true"
        Linea="L1"
        TabOrder="2">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="descripcion"
        Panel="panelgenerales"
        DisplayName="Descripción"
        EditionType="Edit"
        MaxLen="50"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Introduzca un texto que describa este grupo."
        UpdateNeeded="true"
        TabOrder="3">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="ID_CALENDARIO"
        Panel="panelgenerales"
        DisplayName="Calendario"
        EditionType="combo"
        Mandatory="false"
        ValidateAs="Integer"
        ToolTip="Horario que se aplica a la lista"
        External="true"
        UpdateNeeded="true"
        Linea="L5"
        PosColumna="6"
        TabOrder="5">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
          DatabaseName="GENERAL"
          TableName="CAL_CALENDARIO"
          FieldKey="ID_CALENDARIO"
          FieldValue="NOMBRE"
          DefaultKey="NULL"
          DefaultValue="Horario del subsistema"
          FilterSubsistema="true"
          CampoSubsistema="subsistema"
          OrderBy="NOMBRE">
        </table>
      </external>
    </field>

    <!--<field>
      <fattr
        FieldName="grupo_frontel_id"
        Panel="paneltelefonia"
        DisplayName="Grupo frontel"
        EditionType="Combo"
        Mandatory="false"
        ValidateAs="Integer"
        ToolTip="Seleccione el grupo frontel por el que entran las llamadas a este grupo."
        External="true"
        ReadOnly="true"
        UpdateNeeded="false"
        TabOrder="4">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
         DatabaseName="FRONTEL"
         TableName="frontel_phys_group"
         FieldKey="group_code"
         FieldValue="group_name"

    <field>
      <fattr
        FieldName="client_identification_op_code"
        DisplayName="Identificación de Cliente"
        EditionType="Combo"
        Mandatory="false"
        ValidateAs="Integer"
        ToolTip="Operación de identificación de cliente."
        External="true"
        UpdateNeeded="true"
        TabOrder="5">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
        DatabaseName="TIPHONE"
        TableName="operation_types"
        FieldKey="CODE"
        FieldValue="short_name"
        FilterSubsistema="true"
        CampoSubsistema="subsistema"
        DefaultKey="NULL"
        DefaultValue="Ninguno seleccionado">
        </table>
      </external>

    </field>

    <field>
      <fattr
        FieldName="hang_up_op_code"
        DisplayName="Final de gestión"
        EditionType="Combo"
        Mandatory="false"
        ValidateAs="Integer"
        ToolTip="Operación de final de gestión."
        External="true"
        UpdateNeeded="true"
        TabOrder="5">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
        DatabaseName="TIPHONE"
        TableName="operation_types"
        FieldKey="CODE"
        FieldValue="short_name"
        FilterSubsistema="true"
        CampoSubsistema="subsistema"
        DefaultKey="NULL"
        DefaultValue="Ninguno seleccionado">
        </table>
      </external>
    </field>

    <field>
      <fattr
        FieldName="es_de_distribucion"
        Panel="paneltelefonia"
        DisplayName="Distribución"
        EditionType="NONE"
        Mandatory="true"
        ValidateAs="integer"
        ToolTip="Indique si el grupo es de distribución."
        External="false"
        UpdateNeeded="true"
        DefaultValue="1"
        Visible="false"
        TabOrder="6">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="programa_id"
        Panel="panelprogramas"
        DisplayName="Programa de Distribución"
        EditionType="Combo"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Seleccione script para llamadas en cola de distribución."
        External="true"
        UpdateNeeded="true"
        TabOrder="8">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
          DatabaseName="DBQDEVICE"
          TableName="dc_programa"
          FieldKey="programa_id"
          FieldValue="descripcion"
          DefaultKey="POR_DEFECTO"
          DefaultValue="Por defecto"
          FilterSubsistema="true"
          CampoSubsistema="subsistema_id">
        </table>
      </external>
    </field>

    <field>
      <fattr
        FieldName="ringback_programa_id"
        Panel="panelprogramas"
        DisplayName="Programa de ringback"
        EditionType="Combo"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Seleccione script de ringback."
        External="true"
        UpdateNeeded="true"
        TabOrder="10">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
          DatabaseName="DBQDEVICE"
          TableName="dc_programa"
          FieldKey="programa_id"
          FieldValue="descripcion"
          DefaultKey="POR_DEFECTO"
          DefaultValue="Por defecto"
          FilterSubsistema="true"
          CampoSubsistema="subsistema_id">
        </table>
      </external>
    </field>


    <field>
      <fattr
        FieldName="ESTADO"
        Panel="panelgenerales"
        ReadOnly="true"
        DisplayName="Estado"
        EditionType="NONE"
        ValidateAs="BOOLEAN"
        ToolTip="Estado."
        External="false"
        Visible="false"
        UpdateNeeded="false"
        DefaultValue="1"
        TabOrder="4">
      </fattr>
    </field>

  </fields>
</table>

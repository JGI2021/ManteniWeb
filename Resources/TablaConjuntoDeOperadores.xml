<table>

<general
 	DisplayName="Conjunto de operadores"
 	Database="TIPHONE"
 	TableName="CONJUNTO_OPERADORES">
</general>

<panels>
<panel
	id="panelgenerales"
  DisplayName="Datos Generales">  
</panel>

  <panel
    id="PanelSkillSets"
    DisplayName="SkillSets">    
  </panel>
</panels>

<fields>

<field>
<fattr 
	FieldName="id_conjunto_operadores" 
	Panel="panelgenerales"
	DisplayName="Identificador" 
	IsKey="true"
	ReadOnly="true"
	EditionType="AUTOGENERATEDINT" 
	Mandatory="true" 
	ValidateAs="Integer"
	ToolTip="Identificador del conjunto."
	External="false"
	  
	TabOrder="1">
</fattr>		
</field>

<field>
<fattr 
	FieldName="nombre" 
	Panel="panelgenerales"
	DisplayName="Nombre" 
	EditionType="Edit"	
	MaxLen="15"
	Mandatory="true" 
	ValidateAs="Alpha"
	ToolTip="Teclee un nombre para el conjunto."
	External="false"
	Linea="Alias"
  UpdateNeeded="true"
	TabOrder="2">
</fattr>		
</field>

  <field>
    <fattr
      FieldName="Operadores"
      Panel="panelgenerales"
      DisplayName="Operadores"
      EditionType="Table"
      Mandatory="false"
      ToolTip="Operadores incluídos en el conjunto"
      External="true"
      UpdateNeeded="true"
      TabOrder="21">
    </fattr>
    <external>
      <type tipo="TABLEEDIT"/>
      <additionalInfo
        NewRegTitle="Operadores incluídos"
        NewRegType="DosListas"/>
      <table
        DatabaseName="TIPHONE"
        TableName="RELACION_OPERADOR_CONJUNTO"
        TableName2="OP_TABLE"
        Crossfield="OP_CODE"
        Crossfield2="OP_CODE"
        FilterValueLocal="ID_CONJUNTO_OPERADORES"		
        FilterValueExternal="ID_CONJUNTO_OPERADORES"
		SqlCondition="T2.ACTIVO2 IN (1,2)">
      </table>
      <fieldsTable>
        <field2>
          <fattr2
            FieldName="op_code"
            DisplayName="Operador"
            EditionType="Combo"
            Mandatory="true"
            ValidateAs="Integer"
            ReadOnly="true"
            IsKey="true"
            MaxLen="5"
            External="true"
            UpdatedNeeded="true"
            KeyViolationMessage="No se puede añadir.El operador ya estaba previamente añadido."
            Visible="true">
          </fattr2>
          <external2>
            <type2 tipo="table"/>
            <table2
              DatabaseName="TIPHONE"
              TableName="OP_TABLE"
              TableNameFilterUsed="RELACION_OPERADOR_CONJUNTO"
              CrossfieldUsed="OP_CODE"
              CrossfieldUsed2="OP_CODE"
              FilterValueExternalFilter="ID_CONJUNTO_OPERADORES"
              FilterValueLocalFilter="ID_CONJUNTO_OPERADORES"
              FieldKey="op_code"
              FieldValue="OP_NAME"
              DefaultKey="NULL"
              DefaultValue="Ninguno"
              FilterValuesTable="ACTIVO2:1,2:int;"
              FilterSubsistema="true"
              CampoSubsistema="subsistema"
          >
            </table2>
          </external2>
        </field2>

      </fieldsTable>
    </external>
  </field>



  <field>
    <fattr
      FieldName="SkillSets"
      Panel="PanelSkillSets"
      DisplayName="SkillSets"
      EditionType="Table"
      Mandatory="false"
      ToolTip="SkillSets asignados al grupo"
      External="true"
      UpdateNeeded="true"
      TabOrder="22">
    </fattr>
    <external>
      <type tipo="TABLEEDIT"/>
      <additionalInfo
        NewRegTitle="Skilsets asignados"
        NewRegType="DosListas"/>
      <table
        DatabaseName="TIPHONE"
        TableName="RELACION_SKILLSETS_CONJUNTO"
        TableName2="SKILLSETS"
        Crossfield="SKILLSET_ID"
        Crossfield2="SKILLSET_ID"
        FilterValueLocal="ID_CONJUNTO_OPERADORES"
        FilterValueExternal="ID_CONJUNTO_OPERADORES">
      </table>
      <fieldsTable>
        <field2>
          <fattr2
            FieldName="SKILLSET_ID"
            DisplayName="SkillSet"
            EditionType="Combo"
            Mandatory="true"
            ValidateAs="Integer"
            ReadOnly="true"
            IsKey="true"
            MaxLen="5"
            External="true"
            UpdatedNeeded="true"
            KeyViolationMessage="No se puede añadir.El skillset ya ha sido añadido previamente."
            Visible="true">
          </fattr2>
          <external2>
            <type2 tipo="table"/>
            <table2
              DatabaseName="TIPHONE"
              TableName="SKILLSETS"
              TableNameFilterUsed="RELACION_SKILLSETS_CONJUNTO"
              CrossfieldUsed="SKILLSET_ID"
              CrossfieldUsed2="SKILLSET_ID"
              FilterValueExternalFilter="ID_CONJUNTO_OPERADORES"
              FilterValueLocalFilter="ID_CONJUNTO_OPERADORES"
              FieldKey="SKILLSET_ID"
              FieldValue="NOMBRE"
              DefaultKey="NULL"
              DefaultValue="Ninguno"
              FilterValuesTable=""
              FilterSubsistema="true"
              CampoSubsistema="subsistema"
          >
            </table2>
          </external2>
        </field2>

      </fieldsTable>
    </external>
  </field>
</fields>
  
  
</table>

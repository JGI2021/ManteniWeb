<table>

  <general
    DisplayName="Website"
    Database="TIPHONE"
    TableName="CHAT_WEBSITES">
  </general>

  <panels>
    <panel
      id="panelgenerales"
      DisplayName="Generales">
    </panel>
  </panels>

  <fields>

    <field>
      <fattr
        FieldName="idWebSite"
        Panel="panelgenerales"
        DisplayName="Código"
        IsKey="true"
        ReadOnly="true"
        EditionType="AutoGeneratedInt"
        Mandatory="true"
        ValidateAs="Integer"
        ToolTip="Código de operador."
        External="false"
        TabOrder="1">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="Nombre"
        Panel="panelgenerales"
        DisplayName="Alias"
        EditionType="Edit"
        MaxLen="50"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Escribe un Alias para este Website."
        External="false"
        UpdateNeeded="true"
        Linea="alias"
        TabOrder="2">
      </fattr>

    </field>


    <field>
      <fattr
        FieldName="Descripcion"
        Panel="panelgenerales"
        DisplayName="Descripcion"
        EditionType="edit"
        Mandatory="false"
        ValidateAs="Alpha"
        MaxLen="250"
        ToolTip="Descripción del website."
        External="false"
        Linea="alias"
        UpdateNeeded="true"
        TabOrder="3">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="DireccionUrl"
        Panel="panelgenerales"
        DisplayName="Url"
        EditionType="edit"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Escribe la Url del website."
        External="false"
        MaxLen="200"
        UpdateNeeded="true"
        Linea="nombre"
        TabOrder="4">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="REGISTRARNAVEGACION"
        Panel="panelgenerales"
        DisplayName="Registrar navegación"
        EditionType="check"
        Mandatory="false"
        ValidateAs="Boolean"
        ToolTip=""
        External="false"
        MaxLen="1"
        UpdateNeeded="true"
        DefaultValueField="false"
        Linea="nombre"
        MultipleEdicion="true"
        TabOrder="5">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="Activo"
        Panel="panelgenerales"
        DisplayName="Activo"
        EditionType="check"
        Mandatory="false"
        ValidateAs="boolean"
        ToolTip="Marque si desea que esté activo"
        External="false"
        Linea="cuenta"
        UpdateNeeded="true"
        MultipleEdicion="true"
        TabOrder="6">
      </fattr>
    </field>

  </fields>
</table>

<table>

<general
 	DisplayName="Cuentas de e-mail" 
 	Database="TIPHONE"
 	TableName="MY_TEMPLATE">
</general>

<panels>
<panel
	id="panelgenerales"
  DisplayName="Datos Generales">
</panel>
</panels>

    
<fields>

  <field>
  <fattr 
	  FieldName="my_template_id" 
	  Panel="panelgenerales"
	  DisplayName="Identificador" 
	  IsKey="true"
	  ReadOnly="true"
	  EditionType="AUTOGENERATEDINT" 
	  Mandatory="true" 
	  ValidateAs="Integer"
	  ToolTip="Identificador de la cuenta."
	  External="false"
	  TabOrder="1">
  </fattr>		
  </field>
  
  <field>
    <fattr
      FieldName="Nombre"
      Panel="panelgenerales"
      DisplayName="Nombre"
      ReadOnly="false"
      IsKey="false"
      EditionType="Edit"
      Mandatory="true"
      ValidateAs="Alpha"
      ToolTip="Nombre."
      External="false"
      Visible="true"
      MaxLen="100"
      UpdateNeeded="true"
      TabOrder="2">
    </fattr>
  </field>
  
  
  <field>
    <fattr
      FieldName="NOMBRE_HTML"
      Panel="panelgenerales"
      DisplayName="Mensaje para la respuesta automática "
      EditionType="Edit_HTML"
      MaxLen="1000"
      Mandatory="false"
      ValidateAs="Alpha"
      ToolTip="Contenido de la respuesta automática"
      External="false"
      dependencies="false"
      UpdateNeeded="true"
      ContentInURL="true"
      TabOrder="3">
    </fattr>
  </field>
  
  
</fields>
  
</table>

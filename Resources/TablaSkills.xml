</panel>
</panels>

<fields>

<field>
<fattr 
	FieldName="skill_id" 
	Panel="panelgenerales"	
	DisplayName="Identificador" 
	ReadOnly="true"
	IsKey="true"
	EditionType="AUTOGENERATEDINT" 
	Mandatory="true" 
	ValidateAs="Integer"
	ToolTip="Identificador del skill."
	External="false"
	Visible="true"
  UpdateNeeded="false"
	TabOrder="1">
</fattr>		

</field>

  <field>
    <fattr
      FieldName="Nombre"
      Panel="panelgenerales"
      DisplayName="Nombre"
      ReadOnly="false"
      IsKey="false"
      EditionType="Edit"
      Mandatory="true"
      ValidateAs="Alpha"
      ToolTip="Nombre del skill."
      External="false"
      Visible="true"
      MaxLen="200"
      UpdateNeeded="true"
      TabOrder="1">
    </fattr>
  </field>
 

 
  <field>
    <fattr
      FieldName="Operadores"
      Panel="panelgenerales"
      DisplayName="Operadores"
      EditionType="Table"
      Mandatory="false"
      ToolTip="Operadores"
      External="true"
      UpdateNeeded="true"
      TabOrder="21">
    </fattr>
    <external>
      <type tipo="TABLEEDIT"/>
      <additionalInfo
        NewRegTitle="Nueva relación con operador"
        NewRegType="Combo"/>
      <table
        DatabaseName="TIPHONE"
        TableName="OPERADOR_SKILLS"
        TableName2="OP_TABLE"
        Crossfield="OP_CODE"
        Crossfield2="OP_CODE"
        FilterValueLocal="skill_id"
        FilterValueExternal="skill_id"
        CampoSubsistema="subsistema">
      </table>
      <fieldsTable>
        <field2>
          <fattr2
            FieldName="op_code"
            DisplayName="Operador"
            EditionType="Combo"
            Mandatory="true"
            ValidateAs="Integer"
            ReadOnly="true"
            IsKey="true"
            MaxLen="5"
            External="true"
            UpdatedNeeded="true"
            KeyViolationMessage="No se puede añadir.El operador ya estaba previamente añadido."
            Visible="true">
          </fattr2>
          <external2>
            <type2 tipo="table"/>
            <table2
              DatabaseName="TIPHONE"
              TableName="OP_TABLE"
              TableNameFilterUsed="OPERADOR_SKILLS"
              CrossfieldUsed="OP_CODE"
              CrossfieldUsed2="OP_CODE"
              FilterValueExternalFilter="SKILL_ID"
              FilterValueLocalFilter="SKILL_ID"
              FieldKey="op_code"
              FieldValue="OP_NAME"
              DefaultKey="NULL"
              DefaultValue="Ninguno"
              FilterValuesTable="ACTIVO2:1,2:int;"
              FilterSubsistema="true"
              CampoSubsistema="subsistema">
            </table2>
          </external2>
        </field2>
        <field2>
          <fattr2
            FieldName="Prioridad"
            DisplayName="Prioridad"
            EditionType="Edit"
            Mandatory="true"
            ValidateAs="Integer"
            ReadOnly="false"
            IsKey="false"
            MaxLen="2"
            MinValue="1"
            MaxValue="99"
            External="false"
            DefaultValue="1"
            ValorUnicoFila="false"
            UpdatedNeeded="true"
            KeyViolationMessage=""
            Visible="true">
          </fattr2>
        </field2>

      </fieldsTable>
    </external>
  </field>
  
</fields>
</table>
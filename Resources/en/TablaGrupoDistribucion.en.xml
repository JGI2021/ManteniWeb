<table>

  <general
    DisplayName="Distribution ACD Group"
    Database="TIPHONE"
    SubsystemFieldName="subsistema"
    TableName="GRUPO_ACD"
    TableName2="DISTRIBUCION_TELEFONO"
    CrossField2="GRUPO_ACD_ID">
  </general>

  <panels>
    <panel
      id="panelgenerales"
      DisplayName="Distribution Group Data">
    </panel>

    <!--<panel
      id="paneltelefonia"
      DisplayName="Telefonía">
    </panel>-->

    <panel
      id="panelprogramas"
      DisplayName="Scripts">
    </panel>
  </panels>

  <fields>

    <field>
      <fattr
        FieldName="grupo_acd_id"
        Panel="panelgenerales"
        DisplayName="ACD Group Id"
        IsKey="true"
        ReadOnly="true"
        EditionType="AUTOGENERATEDINT"
        Mandatory="true"
        ValidateAs="Integer"
        Linea="L1"
        ToolTip="ACD Group Id"
        TabOrder="1">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="alias"
        Panel="panelgenerales"
        DisplayName="Alias"
        EditionType="Edit"
        MaxLen="10"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Alias is required"
        UpdateNeeded="true"
        Linea="L1"
        TabOrder="2">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="descripcion"
        Panel="panelgenerales"
        DisplayName="Description"
        EditionType="Edit"
        MaxLen="50"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Description"
        UpdateNeeded="true"
        TabOrder="3">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="ID_CALENDARIO"
        Panel="panelgenerales"
        DisplayName="Calendar"
        EditionType="combo"
        Mandatory="false"
        ValidateAs="Integer"
        ToolTip="Calendar"
        External="true"
        UpdateNeeded="true"
        Linea="L5"
        PosColumna="6"
        TabOrder="5">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
          DatabaseName="GENERAL"
          TableName="CAL_CALENDARIO"
          FieldKey="ID_CALENDARIO"
          FieldValue="NOMBRE"
          DefaultKey="NULL"
          DefaultValue="Subsystem calendar"
          FilterSubsistema="true"
          CampoSubsistema="subsistema"
          OrderBy="NOMBRE">
        </table>
      </external>
    </field>
    

    <field>
      <fattr
        FieldName="client_identification_op_code"
        DisplayName="Client identification script"
        EditionType="Combo"
        Mandatory="false"
        ValidateAs="Integer"
        ToolTip="Client identification script"
        External="true"
        UpdateNeeded="true"
        TabOrder="5">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
        DatabaseName="TIPHONE"
        TableName="operation_types"
        FieldKey="CODE"
        FieldValue="short_name"
        FilterSubsistema="true"
        CampoSubsistema="subsistema"
        DefaultKey="NULL"
        DefaultValue="Ninguno seleccionado">
        </table>
      </external>

    </field>

    <field>
      <fattr
        FieldName="hang_up_op_code"
        DisplayName="Final script"
        EditionType="Combo"
        Mandatory="false"
        ValidateAs="Integer"
        ToolTip="Final script"
        External="true"
        UpdateNeeded="true"
        TabOrder="5">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
        DatabaseName="TIPHONE"
        TableName="operation_types"
        FieldKey="CODE"
        FieldValue="short_name"
        FilterSubsistema="true"
        CampoSubsistema="subsistema"
        DefaultKey="NULL"
        DefaultValue="None">
        </table>
      </external>
    </field>

    <field>
      <fattr
        FieldName="es_de_distribucion"
        Panel="paneltelefonia"
        DisplayName="Distribution"
        EditionType="NONE"
        Mandatory="true"
        ValidateAs="integer"
        ToolTip="Distribution"
        External="false"
        UpdateNeeded="true"
        DefaultValue="1"
        Visible="false"
        TabOrder="6">
      </fattr>
    </field>

    <field>
      <fattr
        FieldName="programa_id"
        Panel="panelprogramas"
        DisplayName="Distribution Script"
        EditionType="Combo"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Distribution Script"
        External="true"
        UpdateNeeded="true"
        TabOrder="8">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
          DatabaseName="DBQDEVICE"
          TableName="dc_programa"
          FieldKey="programa_id"
          FieldValue="descripcion"
          DefaultKey="POR_DEFECTO"
          DefaultValue="Default"
          FilterSubsistema="true"
          CampoSubsistema="subsistema_id">
        </table>
      </external>
    </field>

    <field>
      <fattr
        FieldName="ringback_programa_id"
        Panel="panelprogramas"
        DisplayName="Ringback Script"
        EditionType="Combo"
        Mandatory="true"
        ValidateAs="Alpha"
        ToolTip="Ringback Script"
        External="true"
        UpdateNeeded="true"
        TabOrder="10">
      </fattr>
      <external>
        <type tipo="table"/>
        <table
          DatabaseName="DBQDEVICE"
          TableName="dc_programa"
          FieldKey="programa_id"
          FieldValue="descripcion"
          DefaultKey="POR_DEFECTO"
          DefaultValue="Default"
          FilterSubsistema="true"
          CampoSubsistema="subsistema_id">
        </table>
      </external>
    </field>


    <field>
      <fattr
        FieldName="ESTADO"
        Panel="panelgenerales"
        ReadOnly="true"
        DisplayName="Status"
        EditionType="NONE"
        ValidateAs="BOOLEAN"
        ToolTip="Status"
        External="false"
        Visible="false"
        UpdateNeeded="false"
        DefaultValue="1"
        TabOrder="4">
      </fattr>
    </field>

  </fields>
</table>

<table>

<general
 	DisplayName="SkillSet" 
 	Database="TIPHONE"
	SubsystemFieldName="subsistema"
 	TableName="SKILLSETS"
  TableName2="SKILLS"
  CrossField2="SKILL_ID"
>
</general>

<panels>
<panel
	id="panelgenerales"
  DisplayName="Datos Generales">
</panel>
</panels>

<fields>

<field>
<fattr 
	FieldName="skillset_id" 
	Panel="panelgenerales"	
	DisplayName="Identificador" 
	ReadOnly="true"
	IsKey="true"
	EditionType="AUTOGENERATEDINT" 
	Mandatory="true" 
	ValidateAs="Integer"
	ToolTip="Identificador del skillset."
	External="false"
	Visible="true"
  UpdateNeeded="false"
	TabOrder="1">
</fattr>		

</field>

<field>
  <fattr
    FieldName="Nombre"
    Panel="panelgenerales"
    DisplayName="Nombre"
    ReadOnly="false"
    IsKey="false"
    EditionType="Edit"
    Mandatory="true"
    ValidateAs="Alpha"
    ToolTip="Nombre del skill."
    External="false"
    Visible="true"
    MaxLen="200"
    UpdateNeeded="true"
    TabOrder="1"
    Linea="dos">
  </fattr>
</field>

  <field>
    <fattr
      FieldName="Prioridad_Cola"
      Panel="panelgenerales"
      DisplayName="Prioridad en cola"
      ReadOnly="false"
      EditionType="Edit"
      Mandatory="false"
      MaxLen="2"
      DefaultValue="1"
      MinValue="1"
      UpdateNeeded="true"
      TabOrder="2"
      Linea="dos"
      >
      
    </fattr>
  </field>
  <field>
    <fattr
      FieldName="Skills"
      Panel="panelgenerales"
      DisplayName="Skills relacionados"
      EditionType="Table"
      Mandatory="false"
      ToolTip="Skills relacionados"
      External="true"
      UpdateNeeded="true"
      TabOrder="21">
    </fattr>
    <external>
      <type tipo="TABLEEDIT"/>
      <additionalInfo
        NewRegTitle="Nueva relación con skill"
        NewRegType="Combo"    />
      <table
        DatabaseName="TIPHONE"
        TableName="SKILLSETS_SKILL"
        TableName2="SKILLS"
        Crossfield="SKILL_ID"
        Crossfield2="SKILL_ID"
        FilterValueLocal="skillset_id"
        FilterValueExternal="skillset_id"
        CampoSubsistema="subsistema">
      </table>
      <fieldsTable>
        <field2>
          <fattr2
            FieldName="skill_id"
            DisplayName="Skill"
            EditionType="Combo"
            Mandatory="true"
            ValidateAs="Integer"
            ReadOnly="true"
            IsKey="true"
            MaxLen="5"
            External="true"
            UpdatedNeeded="true"
            KeyViolationMessage="No se puede añadir.El skill ya estaba previamente añadido."
            Visible="true">
          </fattr2>
          <external2>
            <type2 tipo="table"/>
            <table2
              DatabaseName="TIPHONE"
              TableName="SKILLS"
              TableNameFilterUsed="SKILLSETS_SKILL"
              CrossfieldUsed="SKILL_ID"
              CrossfieldUsed2="SKILL_ID"
              FieldKey="skill_id"
              FieldValue="nombre"
              DefaultKey="NULL"
              DefaultValue="Ninguno"
              FilterValuesTable=""
              FilterSubsistema="true"
              CampoSubsistema="subsistema"
          
      >
            </table2>
          </external2>
        </field2>

        <field2>
          <fattr2
            FieldName="tipo"
            DisplayName="Tipo"
            EditionType="Combo"
            Mandatory="true"
            ValidateAs="Integer"
            ReadOnly="true"
            IsKey="false"
            MaxLen="5"
            External="true"
            DefaultValue="0"
            ValorUnicoFila="false"
            UpdatedNeeded="true"
            KeyViolationMessage=""
            Visible="true">
          </fattr2>
          <external2>
            <type2 tipo="collection"/>
            <collection2>
              <collectionitem2
                 key="0"
                 value="Excluyente">
              </collectionitem2>
              <collectionitem2
                 key="1"
                 value="Obligatorio">
              </collectionitem2>
              <collectionitem2
                 key="2"
                 value="Opcional">
              </collectionitem2>
            </collection2>
          </external2>
        </field2>
        <field2>
          <fattr2
            FieldName="peso"
            DisplayName="Peso"
            EditionType="Edit"
            Mandatory="true"
            ValidateAs="Integer"
            ReadOnly="false"
            IsKey="false"
            MaxLen="2"
            External="false"
            DefaultValue="0"
            ValorUnicoFila="false"
            UpdatedNeeded="true"
            KeyViolationMessage="Error peso"
            MinValue="0"
            Visible="true">
          </fattr2>
        </field2>

      </fieldsTable>
    </external>
  </field>

</fields>
</table>
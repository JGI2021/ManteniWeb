
@using ManteniWeb.Models



<div class="panel panel-default" style="height:100%;">
    <div class="panel-heading  panel-head bakground-color-info">
        <a data-toggle="collapse" href="#test" class="close" onclick="CerrarPanel('panelAccionUpdate');"><span class="fa fa-close"></span></a>
        <h4 class="panel-head">Actualizar Organización</h4>
    </div>
    <div class="panel-body" id="panel-body-Update">
        <div class="row rowSeparacion20">


            <div class="row">
                <div class="col-lg-12 col-md-12" id="panelNombreUpdate">
                    <label for="nombreOrganizacion">Nombre Organización </label>
                    <input type="text" id="nombreOrgUpdate" class="form-control" value="" required
                           placeholder="Introducir un nombre ..." />
                </div>

            </div>
        </div>

        <div class="row rowSeparacion20" style="text-align:right ;margin-right: 15px !important;">
            <button type="button" class="btn btn-info" onclick="ActualizarDatosOrganizacion();">Actualizar</button>
            <button type="button" class="btn btn-default" onclick="CerrarPanel('panelAccionUpdate')">Salir</button>
        </div>
    </div>
</div>

